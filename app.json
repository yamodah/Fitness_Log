{
  "expo": {
    "name": "Fitness_Log",
    "slug": "Fitness_Log",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/stencil-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#2b2d42"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#2b2d42"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
